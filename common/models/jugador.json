{
  "name": "jugador",
  "plural": "jugadores",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nombre": {
      "type": "string"
    },
    "edad": {
      "type": "string"
    },
    "apodo": {
      "type": "string"
    },
    "foto": {
      "type": "string"
    },
    "posicion": {
      "type": "string"
    },
    "estado": {
      "type": "boolean",
      "default": true
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {},
  "scopes": {
    "novatos": {
      "where": {
        "edad": {
          "lt": 25
        }
      }
    },
    "veteranos": {
      "where": {
        "edad": {
          "gte": 25
        }
      }
    },
    "activos": {
      "where": {
        "estado": "true"
      }
    }
  }
}
